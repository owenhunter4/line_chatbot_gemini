    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "index.js",
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@google/generative-ai": "^0.1.3",
    "axios": "^1.6.5",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.6.0",
    "node-fetch": "^3.3.2",
    "puppeteer": "^19.2.2",
    "urlbox": "^2.1.0",
    "uuid-v4": "^0.1.0"
  },
  "private": true
}
